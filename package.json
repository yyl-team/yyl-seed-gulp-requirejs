{
  "name": "yyl-seed-gulp-requirejs",
  "version": "4.7.3",
  "description": "",
  "engines": {
    "node": ">= 8.0.0"
  },
  "main": "index.js",
  "eslintConfig": {
    "env": {
      "mocha": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "node/no-unpublished-require": [
        0
      ],
      "node/exports-style": [
        "error",
        "module.exports"
      ],
      "node/file-extension-in-import": [
        "error",
        "always"
      ],
      "node/prefer-global/buffer": [
        "error",
        "always"
      ],
      "node/prefer-global/console": [
        "error",
        "always"
      ],
      "node/prefer-global/process": [
        "error",
        "always"
      ],
      "node/prefer-global/url-search-params": [
        "error",
        "always"
      ],
      "node/prefer-global/url": [
        "error",
        "always"
      ],
      "node/prefer-promises/dns": "error",
      "node/prefer-promises/fs": 0,
      "quote-props": [
        "error",
        "consistent"
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ],
      "indent": [
        "warn",
        2
      ]
    }
  },
  "eslintIgnore": [
    "node_modules",
    "/test/case",
    "ui-test",
    "__frag"
  ],
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "concat-with-sourcemaps": "^1.1.0",
    "gulp": "4.0.2",
    "gulp-clean-css": "4.2.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-filter": "5.1.0",
    "gulp-plumber": "1.2.0",
    "gulp-prettify": "0.5.0",
    "gulp-pug": "4.0.1",
    "gulp-rename": "1.4.0",
    "gulp-replace-path": "0.4.0",
    "gulp-uglify": "1.5.3",
    "imagemin": "7.0.1",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-optipng": "7.1.0",
    "imagemin-svgo": "7.0.0",
    "node-watch": "^0.6.3",
    "requirejs": "2.3.6",
    "rev-hash": "3.0.0",
    "through2": "2.0.3",
    "yyl-file-replacer": "0.6.3",
    "yyl-fs": "0.3.3",
    "yyl-inlinesource": "1.2.1",
    "yyl-request": "0.1.1",
    "yyl-seed-response": "0.1.0",
    "yyl-util": "2.3.7"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "eslint": "6.8.0",
    "eslint-plugin-node": "11.0.0",
    "mocha": "7.0.0",
    "yyl-hander": "0.8.2",
    "yyl-os": "0.10.2",
    "yyl-print": "0.7.5",
    "yyl-seed-test-util": "0.4.0",
    "yyl-server": "0.3.4"
  },
  "scripts": {
    "eslint": "eslint ./",
    "eslint:fix": "eslint ./ --fix",
    "test": "mocha ./test/mocha",
    "test:all.case.base": "mocha ./test/mocha/all.case.base.test.js",
    "cba": "node ./test/runner.js all --config ./test/case/base/config.js",
    "cbw": "node ./test/runner.js watch --config ./test/case/base/config.js --proxy",
    "cbac": "node ./test/runner.js all --config ./test/case/base/config.js --isCommit",
    "all:frag.all-base": "node ./test/runner.js all --config ./__frag/all-base/base/config.js",
    "cea": "node ./test/runner.js all --config ./test/case/err-optimize/config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/yyl-seed-gulp-requirejs.git"
  },
  "author": "jackness",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackness1208/yyl-seed-gulp-requirejs/issues"
  },
  "homepage": "https://github.com/jackness1208/yyl-seed-gulp-requirejs#readme"
}
